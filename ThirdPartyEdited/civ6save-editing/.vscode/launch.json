    "version": "0.2.0",
    "configurations": [

        {
            "type": "node",
            "request": "launch",
            "name": "Decompress",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\scripts\\decompress.js",
            "cwd": "${workspaceFolder}\\scripts",
            "args": ["..\\..\\..\\data\\GANDHI 2 3960 BC 16.Civ6Save"]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Save to map table",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\scripts\\savetomaptable.js",
            "cwd": "${workspaceFolder}\\scripts",
            "args": ["..\\..\\..\\data\\GANDHI 20 3240 BC 21.Civ6Save", "test21.md"]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Reveal map",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\scripts\\revealmap.js",
            "cwd": "${workspaceFolder}\\scripts",
            "args": ["..\\..\\..\\data\\GANDHI 2 3960 BC 5.Civ6Save", "GANDHI 2 3960 BC 5 Rev.Civ6Save"]
        }
    ]
}